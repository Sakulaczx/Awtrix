Sub gensky 
	
	For i=0 To 7
		App.drawPixel(Rnd(0,31),Rnd(0,7),Array As Int(51,93,159))
	Next

End Sub

Sub MoonPhase As String
	Dim c,e, b As Int
	Dim jd As Double

	Dim Year As Int = DateTime.GetYear(DateTime.Now)
	Dim Month As Int = DateTime.GetMonth(DateTime.Now)
	Dim Day As Int= DateTime.GetDayOfMonth(DateTime.Now)

	If (Month < 3) Then
		Year = Year - 1
		Month = Month + 12
	End If
	Month = Month + 1

	c = 365.25 * Year
	e = 30.6 * Month
	jd = c + e + Day - 694039.09
	jd = jd / 29.53
	b = jd
	jd = jd - b
	b = jd * 8 + 0.5
	b = Bit.And(b, 7)
	Return b
End Sub